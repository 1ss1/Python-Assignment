    "list1 = []\n",
    "for i in range(2, n):\n",
    "    for ii in range(2, i):\n",
    "        if i % ii == 0 :\n",
    "            break\n",
    "    else : \n",
    "        list1.append(i)\n",
    "print(list1)"
